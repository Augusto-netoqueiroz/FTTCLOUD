{
    "name": "fttelecom/fttelecom",
    "description": "Plataforma SaaS multi‑tenant para comunicação integrada com Asterisk",
    "type": "project",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "spatie/laravel-permission": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
           "psr-4": {
       "Tests\\": "tests/",
       "Database\\Seeders\\": "database/seeders/"
  }
   
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "laravel/tinker": "^2.10"
    }
}
